{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "your-spring-boot-app",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/your-spring-boot-app",
      "dest": "/your-spring-boot-app"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "memory": 1024
    }
  }
}
